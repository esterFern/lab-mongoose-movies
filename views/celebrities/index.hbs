<h2>Celebrities</h2>
<ul>
{{#each celebrities}}
<li>
  <a href="/celebrities/{{this.id}}">{{this.name}}</a>
  <a href="/celebrities/{{this.id}}/edit">Edit</a>
  <form action="/celebrities/{{this.id}}/delete" method="POST">
    <button type="submit">Delete</button>
  </form>
</li>
{{/each}}
</ul>

<a href="/celebrities/new"> Create new</a>

<a href="/">Back</a>