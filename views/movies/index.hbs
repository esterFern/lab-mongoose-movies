<h2>Movies</h2>
<ul>
{{#each movies}}
<li>
  <a href="/movies/{{this.id}}">{{this.title}}</a>
  <a href="/movies/{{this.id}}/edit">Edit</a>
  <form action="/movies/{{this.id}}/delete" method="POST">
    <button type="submit">Delete</button>
  </form>
</li>
{{/each}}
</ul>

<a href="/movies/new"> Create new</a>

<a href="/"> Back</a>